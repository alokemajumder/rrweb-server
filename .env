# Server Configuration
PORT=3000

# AWS Credentials and Region
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Allowed Domains Configuration
# Map each allowed domain to its S3 bucket and preâ€‘shared token.
ALLOWED_DOMAINS='{
  "domain.com": {
    "bucket": "your-bucket-for-domain.com",
    "token": "YOUR_DOMAIN_TOKEN_FOR_DOMAIN_COM"
  },
  "example.com": {
    "bucket": "your-bucket-for-example.com",
    "token": "YOUR_DOMAIN_TOKEN_FOR_EXAMPLE_COM"
  }
}'

# Optionally enable console log recording/replay plugins.
ENABLE_CONSOLE_PLUGIN=true
